<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextButtonSettings">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingVertical">21dp</item>
        <item name="android:textSize">@dimen/text_16</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:drawableTint">@color/gray_white</item>
    </style>
    <style name="head">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:scaleType">center</item>
        <item name="android:background">@color/black</item>
    </style>
    <style name="BackgraundSettings">
        <item name="android:background">@color/white_black</item>
    </style>

    <style name="swith_onoff">
        <item name="android:thumbTint">@color/blue</item>
    </style>
    <style name="serch_line">
        <item name="android:drawableTint">@color/black</item>
    </style>
    <style name="svap_color_b_w">
        <item name="android:src">@drawable/night_back</item>
    </style>
    <style name="TextTrackSearch">
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="TextTrackName">
        <item name="android:textColor">@color/white</item>
    </style>
</resources>